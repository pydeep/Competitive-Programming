#include<bits/stdc++.h>
using namespace std;
int missingNumber(vector<int>& nums) {
    sort(nums.begin(), nums.end());
    int result;
        for(int i=0; i<nums.size(); i++) {
           if(nums[i]!=i && i>0) {
               result = i;
               return result;
           }
        }
        return -1;
    }
};

int main() {
	vector<int> v1 = {0,1};
	
}